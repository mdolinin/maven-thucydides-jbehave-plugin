      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>outputDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description>Location of the file.</description>
        </parameter>
        <parameter>
          <name>packageForStoryStubs</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>Package name for ThucydidesJUnitStory stubs</description>
        </parameter>
        <parameter>
          <name>storiesDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description>Directory with story files</description>
        </parameter>
      </parameters>
      <configuration>
        <outputDirectory implementation="java.io.File" default-value="${project.build.testSourceDirectory}">${project.junit.stories.directory}</outputDirectory>
        <packageForStoryStubs implementation="java.lang.String" default-value="${project.groupId}">${project.junit.stories.package}</packageForStoryStubs>
        <storiesDirectory implementation="java.io.File" default-value="${project.basedir}/src/test/resources/stories">${project.stories.directory}</storiesDirectory>
      </configuration>
    </mojo>
