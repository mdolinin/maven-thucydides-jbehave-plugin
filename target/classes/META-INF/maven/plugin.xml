<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <name>maven-thucydides-jbehave-plugin Maven Mojo</name>
  <description>An Acceptance Testing framework for web applications
        based on WebDriver, and integrating with JUnit and net.thucydides.easyb.</description>
  <groupId>net.thucydides.maven.plugin</groupId>
  <artifactId>maven-thucydides-jbehave-plugin</artifactId>
  <version>0.9.224-SNAPSHOT</version>
  <goalPrefix>thucydides-jbehave</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>generate-soap-steps</goal>
      <description></description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>generate-test-sources</phase>
      <implementation>net.thucydides.maven.plugin.GenerateSoapStepsMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>classesDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description>The directory containing generated classes of the project being tested.
This will be included after the test
classes in the test classpath.</description>
        </parameter>
        <parameter>
          <name>outputDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description>Location of the file.</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>false</editable>
          <description>The Maven Project.</description>
        </parameter>
        <parameter>
          <name>soapServicePackages</name>
          <type>java.lang.String[]</type>
          <required>true</required>
          <editable>true</editable>
          <description>Packages with soap services to scan.</description>
        </parameter>
        <parameter>
          <name>soapStepsPackage</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>Package name for Soap jbehave steps</description>
        </parameter>
        <parameter>
          <name>testClassesDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description>The directory containing generated test classes of the project being tested. This will be included at the
beginning of the test classpath.</description>
        </parameter>
      </parameters>
      <configuration>
        <classesDirectory implementation="java.io.File">${project.build.outputDirectory}</classesDirectory>
        <outputDirectory implementation="java.io.File" default-value="${project.build.directory}/generated-test-sources/soap-steps">${project.soap.steps.stories.directory}</outputDirectory>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
        <testClassesDirectory implementation="java.io.File">${project.build.testOutputDirectory}</testClassesDirectory>
      </configuration>
    </mojo>
    <mojo>
      <goal>generate-steps</goal>
      <description></description>
      <requiresDependencyResolution>test</requiresDependencyResolution>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>process-test-classes</phase>
      <implementation>net.thucydides.maven.plugin.GenerateScenarioStepsMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>classesDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description>The directory containing generated classes of the project being tested. This will be included after the test
classes in the test classpath.</description>
        </parameter>
        <parameter>
          <name>outputDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description>Location of the file.</description>
        </parameter>
        <parameter>
          <name>packageForScenarioSteps</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>Package name for Scenario jbehave steps</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>false</editable>
          <description>The Maven Project.</description>
        </parameter>
        <parameter>
          <name>storiesDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description>Directory with story files</description>
        </parameter>
        <parameter>
          <name>testClassesDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description>The directory containing generated test classes of the project being tested. This will be included at the
beginning of the test classpath.</description>
        </parameter>
      </parameters>
      <configuration>
        <classesDirectory implementation="java.io.File">${project.build.outputDirectory}</classesDirectory>
        <outputDirectory implementation="java.io.File" default-value="${project.build.directory}/generated-test-sources">${project.junit.stories.directory}</outputDirectory>
        <packageForScenarioSteps implementation="java.lang.String" default-value="${project.groupId}">${project.scenario.steps.package}</packageForScenarioSteps>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
        <storiesDirectory implementation="java.io.File" default-value="${project.basedir}/src/test/resources/stories">${project.stories.directory}</storiesDirectory>
        <testClassesDirectory implementation="java.io.File">${project.build.testOutputDirectory}</testClassesDirectory>
      </configuration>
    </mojo>
    <mojo>
      <goal>generate-sources</goal>
      <description></description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
